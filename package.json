{
  "name": "ping-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "description": "A Custom IDE for Intercal Language",
  "author": "Paolo Wattebled, Lucas Besnard, Arthur Guelennoc, Gustave Herve, Arthur Hamard",
  "dependencies": {
    "canvas-confetti": "^1.5.1",
    "i18next": "^23.11.5",
    "monaco-editor": "^0.50.0",
    "openmeteo": "^1.1.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-folder-tree": "^5.1.1",
    "react-i18next": "^14.1.2",
    "react-tabs": "^6.0.2",
    "smalltalk": "^4.1.2",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "main": "dist-electron/main.js",
  "build": {
    "extraResources": [
      {
        "from": "src/backend/ping-1.0-runner",
        "to": "ping-1.0-runner",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "src/backend/ping-1.0-runner.exe",
        "to": "ping-1.0-runner.exe",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "src/backend/ping-1.0-runner-arm",
        "to": "ping-1.0-runner-arm",
        "filter": [
          "**/*"
        ]
      }
    ],
    "files": [
      "dist/",
      "dist-electron/",
      "node_modules/",
      "package.json"
    ],
    "linux": {
      "category": "Utility",
      "icon": "src/assets/appicon.png"
    },
    "win": {
      "icon": "src/assets/appicon.png"
    },
    "mac": {
      "icon": "src/assets/appicon.png"
    }
  }
}
